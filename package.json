{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Modern webpack template for JavaScript",
  "main": "src/index.js",
  "scripts": {
    "setup": "npm install --save-dev webpack webpack-cli webpack-dev-server html-webpack-plugin css-loader style-loader html-loader webpack-merge eslint eslint-config-airbnb-base eslint-plugin-import clean-webpack-plugin mini-css-extract-plugin",
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:dev": "webpack --config webpack/webpack.dev.js",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "clean": "rm -rf dist/"
  },
  "keywords": [
    "webpack",
    "javascript",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
